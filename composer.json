{
    "name": "mazurva/yii2-dadata-suggestions",
    "description": "Подсказки DaData.ru для Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vadim Mazur",
            "email": "mazurva@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package":{
                "name": "hflabs/suggestions-jquery",
                "version": "master",
                "source": {
                    "url":"https://github.com/hflabs/suggestions-jquery",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "hflabs/suggestions-jquery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "mazurva\\widgets\\": ""
        }
    }
}
